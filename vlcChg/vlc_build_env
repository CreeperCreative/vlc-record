CONTRIBS=/usr/win32
PKG_CONFIG_LIBDIR=$CONTRIBS/lib/pkgconfig
PKG_CONFIG_PATH=$CONTRIBS/lib/pkgconfig
CPPFLAGS="-I$CONTRIBS/include -I$CONTRIBS/include/ebml"
CXXCPP=i586-mingw32msvc-cpp
LDFLAGS=-L$CONTRIBS/lib
CC=i586-mingw32msvc-gcc 
CXX=i586-mingw32msvc-g++

export CONTRIBS PKG_CONFIG_LIBDIR PKG_CONFIG_PATH CPPFLAGS CXXCPP LDFLAGS CC CXX
